@article{groth_ab_2017,
	title = {Ab initio},
	volume = {119},
	url = {https://link.aps.org/doi/10.1103/PhysRevLett.119.135001},
	doi = {10.1103/PhysRevLett.119.135001},
	abstract = {In a recent Letter [T. Dornheim et al., Phys. Rev. Lett. 117, 156403 (2016)], we presented the first quantum Monte Carlo (QMC) results for the warm dense electron gas in the thermodynamic limit. However, a complete parametrization of the exchange-correlation free energy with respect to density, temperature, and spin polarization remained out of reach due to the absence of (i) accurate QMC results below θ=kBT/EF=0.5 and (ii) QMC results for spin polarizations different from the paramagnetic case. Here we overcome both remaining limitations. By closing the gap to the ground state and by performing extensive QMC simulations for different spin polarizations, we are able to obtain the first completely ab initio exchange-correlation free energy functional; the accuracy achieved is an unprecedented ∼0.3\%. This also allows us to quantify the accuracy and systematic errors of various previous approximate functionals.},
	number = {13},
	urldate = {2017-09-29},
	journal = {Phys. Rev. Lett.},
	author = {Groth, Simon and Dornheim, Tobias and Sjostrom, Travis and Malone, Fionn D. and Foulkes, W. M. C. and Bonitz, Michael},
	month = sep,
	year = {2017},
	pages = {135001},
	file = {APS Snapshot:/home/groth/.mozilla/firefox/u9v8ouxp.default/zotero/storage/UTEIIZGZ/PhysRevLett.119.html:text/html;Full Text PDF:/home/groth/.mozilla/firefox/u9v8ouxp.default/zotero/storage/ARNPS7Q5/Groth et al. - 2017 - Ab initio.pdf:application/pdf}
}







